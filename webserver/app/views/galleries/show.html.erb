<div class="page-header">
  <%= link_to 'Room', url_with_password(@room), :class => 'btn btn-default' %>
  <h1><%= @room.name %></h1>
</div>

<% if @gallery.videos.first %>
<table>
  <tr>
    <th>Video Name</th>
    <th>Upload Date</th>
  </tr>

  <% @gallery.videos.each do |video| %>
    <tr>
	    <td><%= link_to video.name, room_gallery_video_path(@room, @gallery, video) %></td>
	    <td><%= video.created_at %></td>
	  </tr>
  <% end %>
</table>
<% else %>
  <strong>No videos have been created yet!</strong>
<% end %>
